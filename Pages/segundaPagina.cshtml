@page
@{
    ViewData["Title"] = "Mi Coach";
}

@section Styles {
    <link rel="stylesheet" href="https://unpkg.com/leaflet/dist/leaflet.css" />
    <link rel="stylesheet" href="~/css/segundaPagina.css" />
}
<<a asp-page="/Coach/Info" asp-route-nombre="ronnie-coleman" class="text-decoration-none text-reset">

    <div id="tarjeta" class="card mb-4" style="width: 18rem; cursor: pointer;">
        <img src="data:image/webp;base64," class="card-img-top" alt="ronie">
        <div class="card-body">
            <h5 class="card-title">Ronnie Coleman</h5>
            <p class="card-text">
                <h6>Especialidad:</h6>
                <h6>Puntuación:</h6>
                <div class="rating" data-rating="0">
                    <i class="fas fa-star" data-value="1"></i>
                    <i class="fas fa-star" data-value="2"></i>
                    <i class="fas fa-star" data-value="3"></i>
                    <i class="fas fa-star" data-value="4"></i>
                    <i class="fas fa-star" data-value="5"></i>
                </div>
            </p>
        </div>
    </div>
</a>


@section Scripts {
    <script src="https://unpkg.com/leaflet/dist/leaflet.js"></script>
    <script src="~/js/segundaPagina.js"></script>
}

    <a asp-page="/registroCoach" class="btn btn-primary">¿Quieres ser entrenador/a?</a>

    




